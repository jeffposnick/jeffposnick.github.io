{
  "name": "thirty-four",
  "private": true,
  "engines": {
    "node": ">= 8.0.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.9.0",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@surma/rollup-plugin-off-main-thread": "^1.1.1",
    "firebase-tools": "^7.8.0",
    "fsbin": "^1.0.9",
    "npm-run-all": "^4.1.5",
    "nunjucks": "^3.2.0",
    "rollup": "^1.27.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-typescript": "^1.0.1",
    "tslib": "^1.10.0",
    "typescript": "^3.7.2",
    "workbox-cli": "^5.1.2",
    "workbox-core": "^5.1.2",
    "workbox-expiration": "^5.1.2",
    "workbox-google-analytics": "^5.1.2",
    "workbox-precaching": "^5.1.2",
    "workbox-routing": "^5.1.2",
    "workbox-strategies": "^5.1.2"
  },
  "scripts": {
    "11ty": "eleventy",
    "build": "run-s clean 11ty copy-static bundle-sw inject-manifest",
    "bundle-sw": "rollup -c",
    "clean": "fsbin remove build",
    "copy-static": "fsbin copy assets build/assets && fsbin copy _posts/_data build/_posts/_data && fsbin copy _posts/_includes build/_posts/_includes",
    "deploy": "run-s build firebase-hosting",
    "firebase-hosting": "firebase deploy --only=hosting",
    "inject-manifest": "workbox injectManifest",
    "serve": "firebase serve --only=hosting"
  }
}
