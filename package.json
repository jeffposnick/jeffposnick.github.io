{
  "name": "thirty-four",
  "private": true,
  "engines": {
    "node": ">= 8.0.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.6.0",
    "@11ty/eleventy-plugin-rss": "^1.0.5",
    "firebase-tools": "^6.2.2",
    "fsbin": "^1.0.7",
    "pretty-bytes": "^5.1.0",
    "workbox-cli": "^4.0.0-beta.0"
  },
  "scripts": {
    "11ty-debug": "DEBUG=Eleventy* eleventy",
    "11ty": "eleventy",
    "build": "npm run clean && npm run 11ty && npm run copy-static && npm run workbox",
    "clean": "fsbin remove build",
    "copy-static": "fsbin copy src/assets build/assets && fsbin copy src/content/_data build/_sw/_data",
    "deploy": "npm run build && firebase deploy --only=hosting",
    "serve": "firebase serve --only=hosting",
    "workbox": "workbox injectManifest"
  }
}
